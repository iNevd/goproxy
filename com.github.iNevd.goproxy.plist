<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
    <dict>
        <key>Label</key>
        <string>com.github.iNevd.goproxy</string>
        <key>KeepAlive</key>
        <true/>
        <key>RunAtLoad</key>
        <true/>
        <key>ProgramArguments</key>
        <array>
            <string>`full path`/goproxy.launchctl</string>
        </array>
        <key>StandardOutPath</key>
        <string>`full path`/log/1.log</string>
        <key>StandardErrorPath</key>
        <string>`full path`/log/2.log</string>
    </dict>
</plist>
